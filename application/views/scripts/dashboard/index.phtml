                $("#pickup_address").val(add);


            }



        });
    }





    function change_validate(value)  {
        if(value=='1' || value=='3')
        {
            $(".dtd").show();
            $(".dtp").hide();
            $("#delivery_address_star").show();
            $("#pickup_address_star").hide();

            $("#assigned_driver_2_id").val("");
