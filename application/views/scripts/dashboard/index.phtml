    function currency_format(n) {
        return n.toFixed(2).replace(/./g, function(c, i, a) {
            return i > 0 && c !== "." && (a.length - i) % 3 === 0 ? "," + c : c;
        });
    }

    function change_amounts(value)  {
        due_amount = $("#due_amount1").val() || 0;
        additional_charges = $("#additional_charges").val() || 0;
        expenses = $("#expenses").val() || 0;

