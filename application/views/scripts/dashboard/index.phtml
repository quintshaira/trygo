    <script>
        function validate1()
        {
            var recieved_amount = $("#recieved_amount").val().trim()*1;
            var due_amount1 = $("#due_amount1").val().trim()*1;
            var recieved_remarks = $("#recieved_remarks").val().trim();
            var error = '';

            if(recieved_amount > 0)
            {
                $("#err_remmited_amount").html('&nbsp;');
                if(recieved_amount < due_amount1){
                    if(!recieved_remarks)
                    {
                        error += '1';
                        $("#err_recieved_remarks").html('Please add remarks');
                    }else{
                        $("#err_recieved_remarks").html('&nbsp;');
                    }
                }else{
                    $("#err_recieved_remarks").html('&nbsp;');
                    $("#err_remmited_amount").html('&nbsp;');
                }
            }else{
                error += '3';
                $("#err_remmited_amount").html('Please add Remitted Amount');
            }

            if(error)
            return false;
        }
    </script>
